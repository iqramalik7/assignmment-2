@{
}
<head>
    <link rel="stylesheet" href="~/css/table.css">
</head>

@using web_assignment2.Models;

<body>
    <table>
        <tr>
          <th>id</th>
          <th>name</th>
          <th>E-Mail</th>
          <th>Phone</th>
          <th>Gender</th>
          <th>Department</th>
          <th colspan="3">Message</th>
        </tr>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.id</td>
                    <td>@item.name</td>
                    <td>@item.email</td>
                    <td>@item.phone</td>
                    <td>@item.gender</td>
                    <td>@item.dep</td>
                    <td>@item.message</td>
                </tr>
            }
</table>
</body>